<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.azure.home.todolist">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.azure.home.todolist.MainActivity"
            android:screenOrientation="portrait"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.azure.home.todolist.SignupActivity"
                android:screenOrientation="portrait"
            />
        <activity android:name="com.azure.home.todolist.SubjectActivity"
            android:screenOrientation="portrait"
            />
        <activity android:name="com.azure.home.todolist.SubitemActivity"
            android:screenOrientation="portrait"
            />
        <activity android:name="com.azure.home.todolist.SubRegister"
            android:screenOrientation="portrait"
            />
        <activity android:name="com.azure.home.todolist.TodoActivity"
            android:screenOrientation="portrait"
            />
        <activity android:name="com.azure.home.todolist.TodoitemActivity"
            android:screenOrientation="portrait"
            />
        <activity android:name="com.azure.home.todolist.Todoenroll"
            android:screenOrientation="portrait"
            />
        <activity android:name="com.azure.home.todolist.SubFixActivity"
            android:screenOrientation="portrait"
            />
        <activity android:name="com.azure.home.todolist.TodoFixActivity"
            android:screenOrientation="portrait"
            />
        <activity android:name="com.azure.home.todolist.Todohiddenshow"
            android:screenOrientation="portrait"
            ></activity>
    </application>

</manifest>